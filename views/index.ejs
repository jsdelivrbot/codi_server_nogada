<!DOCTYPE html>
<!-- saved from url=(0040)http://bootstrapk.com/examples/carousel/ -->
<html lang="ko">

<head>
    <%- include('load'); %>
        <link href="/CSS/cover.css" rel="stylesheet">
        <style>
            a {
                text-align: left;
            }
            
            body {
                overflow: visible !important;
                padding: 0 !important;
            }
            
            .UIInput-label {
                font-size: small;
            }
            
            .loginBtn {
                margin-top: 30px !important;
            }

        </style>
</head>


<body>
    <%- include('navbar', {search_on: false,active:0}); %>
        <div class="site-wrapper">
            <div class="site-wrapper-inner" style="padding-top: 100px;vertical-align: initial;">
                <div class="cover-container">
                    <div class="masthead clearfix"> </div>
                    <div class="inner cover">
                        <h1 class="cover-heading" style="margin-bottom: 25px;">
                            <img src="/image/textlogo.png" style="width: 200px;">
                        </h1>
                        <form action="./search" method="post">
                            <div class="input-group" style="margin-left: 30px;margin-right: 30px;">

                                <input name="data" id="searchBar" type="text" class="form-control SearchBar" placeholder="Search for..." style="font-size: larger;height: 40px;">
                                <span class="input-group-btn" style="">
                                <button  type="submit" class="btn btn-defaul SearchButton" type="button" style="width: 50px;background-color: #FFA000;height: 40px;">
                                    <img src="/image/search1.png" style="width: 100%;height: 100%;">
                                </button>
                            </span>

                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

</body>

</html>
